{ 
	"manifest_version": 2,
	"name": "StreamWeaver",
	"version": "0.0.2",
	"icons": {
		"16":"src/img/icon16.png",
		"48":"src/img/icon48.png",
		"128":"src/img/icon16.png"
	},
	"browser_action": {
		"default_icon": "src/img/icon48.png",
		"default_popup": "src/popup.html"
	},
	"background": {
		"scripts": [
			"src/js/jquery-1.10.2.min.js",
			"src/js/notify.js",
			"src/js/bg.js"
		]
	},
	"permissions": [
		"https://api.stre.am:9443/v1/stream",
		"notifications"
	]
}
